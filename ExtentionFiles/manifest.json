{
  "manifest_version": 3,
  "name": "KissanimeExtention",
  "version": "1.0.0",

  "description": "Makes you use the server with the least ads on kissanimefree.cc",

  "host_permissions": [
    "https://kissanimefree.cc/*"
  ],


  "content_scripts": [{
    "matches": ["*://*.kissanimefree.cc/*"],
    "js": ["jquery-1.12.4.min.js", "foreground.js"],
    "run_at": "document_end",
    "all_frames": true
  }],

  "icons": {
    "16": "./images/logo16.png",
    "32": "./images/logo32.png",
    "48": "./images/logo48.png",
    "128": "./images/logo128.png"
  },
  "action": {
    "default_icons": {
      "16": "./images/logo16.png",
      "32": "./images/logo32.png",
      "48": "./images/logo48.png",
      "128": "./images/logo128.png"
    }
  }
}
